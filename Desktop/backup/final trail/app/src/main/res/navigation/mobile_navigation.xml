<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.gcit.jobhutfinal.ui.home.HomeFragment"
        android:label="JobHut"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.gcit.jobhutfinal.ui.gallery.GalleryFragment"
        android:label="@string/menu_forgot"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.gcit.jobhutfinal.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_change"
        tools:layout="@layout/fragment_slideshow" >
        <action
            android:id="@+id/action_nav_slideshow_self"
            app:destination="@id/nav_slideshow" />
    </fragment>
</navigation>